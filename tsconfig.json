{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "newLine": "LF",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "pretty": true,
    "strict": true,
    "target": "es2017"
  },
  "compileOnSave": false
}
